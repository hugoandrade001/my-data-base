SELECT * FROM NOTAS_FISCAIS

SELECT * FROM TABELA_DE_VENDEDORES

SELECT NT.MATRICULA, TV.NOME, COUNT(*) AS N_VENDAS 
FROM NOTAS_FISCAIS NT 
INNER JOIN TABELA_DE_VENDEDORES TV
ON NT.MATRICULA = TV.MATRICULA
GROUP BY  NT.MATRICULA, TV.NOME

SELECT INF.CODIGO_DO_PRODUTO, TP.NOME_DO_PRODUTO, SUM(INF.QUANTIDADE) AS QUANTIDADE FROM ITENS_NOTAS_FISCAIS  INF
INNER JOIN TABELA_DE_PRODUTOS TP 
ON INF.CODIGO_DO_PRODUTO = TP.CODIGO_DO_PRODUTO
GROUP BY INF.CODIGO_DO_PRODUTO, TP.NOME_DO_PRODUTO HAVING SUM(INF.QUANTIDADE) < 394000 
ORDER BY SUM(INF.QUANTIDADE) DESC;

SELECT DISTINCT TC.CPF AS CPF_COMPRADOR, TC.NOME AS NOME, NT.CPF AS CPF_NOTAFISCAL
FROM TABELA_DE_CLIENTES TC LEFT JOIN NOTAS_FISCAIS NT
ON TC.CPF = NT.CPF

SELECT * FROM TABELA_DE_CLIENTES

SELECT * FROM TABELA_DE_VENDEDORES 





SELECT TV.NOME AS NOME_VENDEDOR, TC.NOME AS NOME_CLIENTE, TC.BAIRRO AS BAIRRO_CLIENTE, TV.BAIRRO AS BAIRRO_VENDEDOR
FROM TABELA_DE_VENDEDORES TV RIGHT JOIN  TABELA_DE_CLIENTES TC
ON TV.BAIRRO = TC.BAIRRO 
